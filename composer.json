{
    "name": "sirbrillig/spies",
    "description": "Easier spies, stubs, and mocking for PHP tests",
    "type": "library",
    "keywords": ["phpunit", "mocking", "stubs", "spies", "spy"],
    "homepage": "https://github.com/sirbrillig/spies",
    "require": {
        "php": ">=5.3.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "phpunit/phpunit": "^5.2",
        "antecedent/patchwork": "^2.0.0"
    },
    "scripts": {
        "test": ["vendor/bin/phpunit"]
    },
    "license": "MIT",
    "autoload": {
        "classmap": [
          "src/"
        ],
        "psr-0": {
            "Spies": "src/"
        },
        "files": [ "src/Spies/functions.php" ]
    },
    "authors": [
        {
            "name": "Payton Swick",
            "email": "payton@foolord.com"
        }
    ]
}
